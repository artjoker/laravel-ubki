{
    "name": "artjoker/laravel-ubki",
    "description": "Package for integration UBKI",
    "authors": [
        {
            "name": "Volodymyr Taranenko",
            "email": "taranenko@vt2.com.ua"
        }
    ],
    "homepage": "https://github.com/artjoker/laravel-ubki",
    "keywords": ["Laravel", "Ubki"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5|^8.0|^9.0",
        "mockery/mockery": "^1.1|^1.5.0",
        "orchestra/testbench": "~3.0|^4.0|^5.0|^6.0|^7.0",
        "sempro/phpunit-pretty-print": "^1.0|^1.4.0"
    },
    "autoload": {
        "psr-4": {
            "Artjoker\\LaravelUbki\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artjoker\\LaravelUbki\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Artjoker\\LaravelUbki\\LaravelUbkiServiceProvider"
            ],
            "aliases": {
                "LaravelUbki": "Artjoker\\LaravelUbki\\Facades\\LaravelUbki"
            }
        }
    }
}
